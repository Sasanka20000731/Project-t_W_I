            <td>
                @Html.DisplayFor(modelItem => item.Active)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.tblInventoryData.SerialNumber)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.mWarrentyId }) |
                @Html.ActionLink("Details", "Details", new { id = item.mWarrentyId }) |
