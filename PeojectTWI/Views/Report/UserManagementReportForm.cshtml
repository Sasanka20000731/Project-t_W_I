
@{
    ViewBag.Title = "UserManagementReportForm";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>User Management Report Form</h2>


<div class="row containerBlock" id="form" ng-app="myapp" ng-controller="MyController" ng-init="LoadData()">

    <div class="row containerBlock">
        <div class="col-4">
            <label>From</label>
            <input type="date" name="UserReportFormDate" id="UserReportFormDate" />
        </div>
        <div class="col-4">
            <label>To</label>
            <input type="date" name="UserReportToDate" id="UserReportToDate" />
        </div>
        <div class="col-4">
            <label for="droReportType" class="col-md-2 control-label">Report Type</label>
            <select id="droReportType" name="droReportType" ng-model="droReportType" class="form-control" required>
                <option value="">Select User Level</option>
                <option ng-repeat="item in droReportTypeList" value="{{item.ReportTypeId}}">{{item.ReportType}}</option>
            </select>
        </div>
    </div>

    <div class="row  containerBlock">
        <div>
            <button class="custom-button" style="width:100%" ng-click="Search">Search</button>
        </div>
        <div>
            <button class="custom-button" style="width:100%" ng-click="Dewonload()">Download</button>
        </div>

    </div>

