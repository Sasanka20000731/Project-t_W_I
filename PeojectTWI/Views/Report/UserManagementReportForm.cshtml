<style>
    .bold-item {
        font-weight: bold;
        font-size: 18px;
    }

    .Strong-item {
        font-weight: bold;
        font-size: 24px;
    }

    .normal-item {
        font-size: 18px;
    }

    .custom-button {
        height: 90%;
        background-color: #1a2c34;
        border-radius: 10px;
        float: left;
        border: 0px;
        color: #fff;
        width: 100%;
        -webkit-transition-duration: 0.9s;
        -webkit-transition-timing-function: linear;
        box-shadow: 0px 0 0 #2e4f5e inset;
    }

        .custom-button:hover {
            box-shadow: 700Px 0 0 #2e4f5e inset;
        }
</style>




