
@{
    ViewBag.Title = "RemainingStock";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>RemainingStock</h2>

<div class="form-horizontal" ng-app="myapp" ng-controller="MyController" ng-init="LoadStockData()" style="margin-bottom:100px;">


   
    <div class="wrapper">




        <div class="content">


            <div class="container-fluid">

                <div class="row">
                    <div class="col-12">
                        <div class="row g-2">

                            <div class="mb-3 col-md-6">
                                <h1>Brand : {{BrandName}}</h1>


                            </div>
                            <div class="mb-3 col-md-6">
                                <h1>Product : {{ProductName}}</h1>
                            </div>
                        </div>
                        <div class="card">

                            <div class="card-body">

                                <br>


                                <div class="tab-content">
                                    <div class="tab-pane show active" id="form-row-preview">
                                        <form method="POST" enctype="multipart/form-data">
                                            <div class="row g-2">

                                                <div class="mb-3 col-md-6">
                                                    <div class="row g-2">
                                                        <div class="mb-1 col-md-3">
                                                            <label for="example-palaceholder" class="form-label">Vender Name:</label>
                                                        </div>
                                                        <div class="mb-3 col-md-6">
                                                            <label for="example-palaceholder" class="form-label">{{VendorName}}</label>

                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="mb-3 col-md-6">
                                                    <div class="row g-2">
                                                        <div class="mb-1 col-md-3">
                                                            <label for="example-palaceholder" class="form-label">Vender Contact:</label>
                                                        </div>
                                                        <div class="mb-3 col-md-6">
                                                            <label for="example-palaceholder" class="form-label">{{VendorContact}}</label>

                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row g-2">

                                                <div class="mb-3 col-md-6">
                                                    <div class="row g-2">
                                                        <div class="mb-1 col-md-3">
                                                            <label for="example-palaceholder" class="form-label">Vender Address</label>
                                                        </div>
                                                        <div class="mb-3 col-md-6">
                                                            <label for="example-palaceholder" class="form-label">{{VendorAddress}}</label>

                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="mb-3 col-md-6">
                                                    <div class="row g-2">
                                                        <div class="mb-1 col-md-3">
                                                            <label for="example-palaceholder" class="form-label">Registered Date</label>
                                                        </div>
                                                        <div class="mb-3 col-md-6">
                                                            <label for="example-palaceholder" class="form-label">{{RegisteredDate}}</label>

                                                        </div>
                                                    </div>
                                                </div>
                                            </div>

                                            @*<div class="row g-2">

                                                <div class="mb-3 col-md-6">
                                                    <div class="row g-2">
                                                        <div class="mb-1 col-md-3">
                                                            <label for="example-palaceholder" class="form-label">Vender Name:</label>
                                                        </div>
                                                        <div class="mb-3 col-md-6">
                                                            <label for="example-palaceholder" class="form-label">Saaaaaaa</label>

                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="mb-3 col-md-6">
                                                    <div class="row g-2">
                                                        <div class="mb-1 col-md-3">
                                                            <label for="example-palaceholder" class="form-label">Vender Name:</label>
                                                        </div>
                                                        <div class="mb-3 col-md-6">
                                                            <label for="example-palaceholder" class="form-label">PSaaaaaaa</label>

                                                        </div>
                                                    </div>
                                                </div>
                                            </div>*@





                                        </form>
                                    </div> 
                                </div>

                            </div>
                        </div> 
                    </div>
                </div>
            </div>

            <div class="col-12">
                <div class="card">
                    <div class="card-body">
                        <h4 class="header-title">Order Details</h4>
                        <br>
                        <div class="tab-pane show active" id="hoverable-rows-preview">
                            <div class="table-responsive-sm">
                                <table class="table table-hover table-centered mb-0">
                                    <thead>
                                        <tr>
                                            <th>No.</th>
                                            <th>Brand Name </th>
                                            <th>Product Name</th>
                                            <th>Vendot Name</th>
                                            <th>PerchesedCount</th>
                                            <th>RemainingCount</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr ng-repeat="item in Result">
                                            <td>{{$index + 1}}</td>
                                            <td>{{ item.BrandName }}</td>
                                            <td>{{ item.ProductName }}</td>
                                            <td>{{ item.VendorName }}</td>
                                            <td>{{ item.PerchesedCount }}</td>
                                            <td>{{ item.RemainingCount }}</td>
                                        </tr>

                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>


            </div>

        </div>





        <!-- ============================================================== -->
        <!-- End Page content -->
        <!-- ============================================================== -->


    </div>





</div>

<style>
    .bold-item {
        font-weight: bold;
